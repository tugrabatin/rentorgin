# STAGING ENVIRONMENT CONFIGURATION
# For testing before production deployment

# Database
DATABASE_URL="postgresql://user:password@staging-db.example.com:5432/rentorgin_staging?schema=public"

# API
API_PORT=3001
API_URL=https://staging-api.rentorgin.com

# Frontend
NEXT_PUBLIC_API_URL=https://staging-api.rentorgin.com/api/v1

# Authentication
JWT_SECRET=staging-secret-change-me
JWT_EXPIRATION=15m

# OpenAI (Use test API key or same as prod)
OPENAI_API_KEY=sk-staging-your-key

# Monitoring
SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
SENTRY_ENVIRONMENT=staging

# Application
APP_ENV=staging
APP_VERSION=0.3.0
NODE_ENV=production

# Rate Limiting (More lenient for testing)
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=200

















