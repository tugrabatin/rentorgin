{
  "extends": "../../apps/api/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "lib": ["ES2020"],
    "outDir": "../../.netlify/functions",
    "rootDir": ".",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": ["api.ts"],
  "exclude": ["node_modules"]
}
